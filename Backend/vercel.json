{
 "version": 2,
 "builds": [
  {
   "src": "index.js",
   "use": "@vercel/node",
   "config": { "includeFiles": ["dist/**"] }
  }
 ],
 "routes": [
  {
   "src": "/(.*)",
   "dest": "index.js",
   "headers": {
        "Access-Control-Allow-Origin": [
          "https://mern-stack-portfolio-git-main-zain-abros-projects.vercel.app",
          "https://mern-stack-portfolio-dsn8qvrdf-zain-abros-projects.vercel.app"
        ],
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS,PATCH",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, Cache-Control, Set-Cookie, Accept, X-Requested-With",
        "Strict-Transport-Security": "max-age=63072000; includeSubDomains; preload",
        "X-Frame-Options": "DENY",
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "strict-origin-when-cross-origin"
      }
  }
 ]
}